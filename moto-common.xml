<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="moto-common"
        fetch="https://github.com/moto-common/" />
    <remote name="hanoip"
        fetch="https://github.com/moto-hanoip/" />
    <remote name="aospa"
        fetch="https://github.com/AOSPA/" />
    <remote name="aospa-vendor"
        fetch="https://github.com/ThankYouMario/" />
    <!-- CLO -->
    <remote name="clo-la"
            fetch="https://git.codelinaro.org/clo/la"
            sync-c="true"
            sync-tags="false" />

    <!-- moto-common main tree -->
    <include name="common.xml"/>
    <!-- misc trees -->
    <include name="misc.xml"/>
    <!-- device trees -->
    <include name="hanoip.xml"/>
    <!-- soc specific trees -->
    <include name="qcom.xml"/>
</manifest>
